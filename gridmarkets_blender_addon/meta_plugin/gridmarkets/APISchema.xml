                <Type>ENUM</Type>
                <Items>
                    <Item>
                        <Key>CYCLES</Key>
                        <DisplayName>Cycles</DisplayName>
                        <Description>Blender Cycles.</Description>
                    </Item>
                    <Item>
                        <Key>EEVEE</Key>
                        <DisplayName>Eevee</DisplayName>
                        <Description>Blender Eevee.</Description>
                    </Item>
                </Items>
                <DefaultValue>CYCLES</DefaultValue>
            </Attribute>
            <Transitions>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_80_CYCLES_PROJECT</ProjectAttributeId>
                    <TransitionFormula>CYCLES</TransitionFormula>
                </Transition>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_80_EEVEE_PROJECT</ProjectAttributeId>
                    <TransitionFormula>EEVEE</TransitionFormula>
                </Transition>
            </Transitions>
            <CompatibleJobDefinitions></CompatibleJobDefinitions>
        </ProjectAttribute>

        <ProjectAttribute id="BLENDER_2_79_MAIN_PROJECT_FILE">
            <Attribute>
                <Key>MAIN_FILE</Key>
                <DisplayName>Main project file</DisplayName>
                <Description>The main .blend file for your project.</Description>
                <Type>STRING</Type>
            </Attribute>
            <Transitions>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_79_RENDER_ENGINE</ProjectAttributeId>
                    <TransitionFormula>(.*)(.blend)$</TransitionFormula>
                </Transition>
            </Transitions>
            <CompatibleJobDefinitions></CompatibleJobDefinitions>
        </ProjectAttribute>

        <ProjectAttribute id="BLENDER_2_80_MAIN_PROJECT_FILE">
            <Attribute>
                <Key>MAIN_FILE</Key>
                <DisplayName>Main project file</DisplayName>
                <Description>The main .blend file for your project.</Description>
                <Type>STRING</Type>
            </Attribute>
            <Transitions>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_80_RENDER_ENGINE</ProjectAttributeId>
                    <TransitionFormula>(.*)(.blend)$</TransitionFormula>
                </Transition>
            </Transitions>
            <CompatibleJobDefinitions></CompatibleJobDefinitions>
        </ProjectAttribute>

        <ProjectAttribute id="BLENDER_VERSION">
            <Attribute>
                <Key>VERSION</Key>
                <DisplayName>Version</DisplayName>
                <Description>The version of Blender.</Description>
                <Type>ENUM</Type>
                <Subtype>PRODUCT_VERSIONS</Subtype>
                <SubtypeKwargs>
                    <SubtypeKwarg>
                        <Key>PRODUCT</Key>
                        <Value>blender</Value>
                    </SubtypeKwarg>
                </SubtypeKwargs>
            </Attribute>
            <Transitions>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_79_MAIN_PROJECT_FILE</ProjectAttributeId>
                    <TransitionFormula>^([2]\.)([1-7]\d)</TransitionFormula>
                </Transition>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_80_MAIN_PROJECT_FILE</ProjectAttributeId>
                    <TransitionFormula>.*</TransitionFormula>
                </Transition>
            </Transitions>
            <CompatibleJobDefinitions></CompatibleJobDefinitions>
        </ProjectAttribute>

