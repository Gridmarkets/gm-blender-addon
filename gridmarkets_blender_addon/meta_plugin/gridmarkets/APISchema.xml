                </Attribute>
                <Optional>True</Optional>
                <InferenceSources>
                    <InferenceSource>USER_DEFINED</InferenceSource>
                </InferenceSources>
            </JobAttribute>

            <JobAttribute>
                <Attribute>
                    <Key>output_format</Key>
                    <DisplayName>Output Format</DisplayName>
                    <Description>The output format to use when rendering the scene.</Description>
                    <Type>STRING</Type>
                    <DefaultValue>PNG</DefaultValue>
                </Attribute>
                <Optional>True</Optional>
                <InferenceSources>
                    <InferenceSource>USER_DEFINED</InferenceSource>
                </InferenceSources>
            </JobAttribute>

        </JobDefinition>

    </JobDefinitions>

