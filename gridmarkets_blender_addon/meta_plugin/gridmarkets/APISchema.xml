        <ProjectAttribute id="BLENDER_2_79_RENDER_ENGINE">
            <Attribute>
                <Key>BLENDER_2_79_RENDER_ENGINE</Key>
                <DisplayName>Render Engine</DisplayName>
                <Description>The render engine the project is set to use.</Description>
                <Type>ENUM</Type>
                <Items>
                    <Item>
                        <Key>INTERNAL</Key>
                        <DisplayName>Blender Internal</DisplayName>
                        <Description>The internal blender engine.</Description>
                    </Item>
                    <Item>
                        <Key>CYCLES</Key>
                        <DisplayName>Cycles</DisplayName>
                        <Description>Blender Cycles.</Description>
                    </Item>
                </Items>
                <DefaultValue>CYCLES</DefaultValue>
            </Attribute>
            <Transitions>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_79_CYCLES_PROJECT</ProjectAttributeId>
                    <TransitionFormula>CYCLES</TransitionFormula>
                </Transition>
                <Transition>
                    <ProjectAttributeId>BLENDER_2_79_INTERNAL_PROJECT</ProjectAttributeId>
                    <TransitionFormula>INTERNAL</TransitionFormula>
                </Transition>
            </Transitions>
